{
  "name": "comsave/salesforce-outbound-message-bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "This bundle allows you to easily process outbound messages sent by Salesforce.",
  "autoload": {
    "psr-4": {
      "Comsave\\": "src/Comsave/"
    }
  },
  "provide": {
    "ext-mongo": "*"
  },
  "require": {
    "php": ">=7.2.0",
    "ext-soap": "^7.3",
    "comsave/salesforce-mapper-bundle": "^3.0.6",
    "doctrine/mongodb-odm": "^1.1",
    "symfony/monolog-bundle": "^3.4|^4.0",
    "symfony/polyfill-apcu": "^1.12"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^3.0|^4.0",
    "phpunit/php-code-coverage": "^6.0",
    "phpunit/phpunit": "^7.0"
  },
  "config": {
    "sort-packages": true
  }
}
