{
  "name": "comsave/salesforce-outbound-message-bundle",
  "description": "Salesforce outbound message handler. Will process the outbound message sent by Salesforce and save all the changes to your database.",
  "provide" : {
    "ext-mongo": "*"
  },
  "require": {
    "php": ">=7.2.0",
    "friendsofsymfony/rest-bundle": "^2.3",
    "doctrine/doctrine-bundle": "^1.9",
    "comsave/salesforce-mapper-bundle": "^3.0.6",
    "gedmo/doctrine-extensions": "^2.4",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/event-dispatcher": "^3.4.9",
    "doctrine/mongodb-odm": "^1.2",
    "symfony/symfony": "^3.4.9"
  },
  "require-dev": {
    "phpunit/php-code-coverage": "^6.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4" : {"": "src/"}
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Resources\\": "Tests/Resources"
    }
  }
}